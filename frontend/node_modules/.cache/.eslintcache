div className=\"mt-4\">\r\n          <ul>\r\n            <li>\r\n              <h2>Order Summary</h2>\r\n            </li>\r\n            <li>\r\n              <div className=\"mb-2\">\r\n                <div className=\"\">Items Price:</div>\r\n                <div className=\"\">${cart.itemsPrice.toFixed(2)}</div>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div className=\"mb-2\">\r\n                <div className=\"\">Shipping Price</div>\r\n                <div className=\"\">${cart.shippingPrice.toFixed(2)}</div>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div className=\"\">\r\n                <div className=\"\">Tax</div>\r\n                <div className=\"\">${cart.taxPrice.toFixed(2)}</div>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <div className=\"mt-3\">\r\n                <h4>Order Total</h4>\r\n                <h4 className=\"fs-3 text-danger\">\r\n                  ${cart.totalPrice.toFixed(2)}\r\n                </h4>\r\n              </div>\r\n            </li>\r\n            <li>\r\n              <button\r\n                type=\"button\"\r\n                onClick={placeOrderHandler}\r\n                className=\"btn btn-dark mt-4 btn-lg\"\r\n                disabled={cart.cartItems.length === 0}\r\n              >\r\n                Place Order\r\n              </button>\r\n            </li>\r\n            {loading && <LoadingBox></LoadingBox>}\r\n            {error && <MessageBox variant=\"danger\">{error}</MessageBox>}\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PlaceOrderScreen;\r\n","G:\\e-commerce-mern-3\\frontend\\src\\action\\userActions.js",["426"],"import axios from \"axios\";\r\nimport {\r\n  USER_DELETE_FAIL,\r\n  USER_DELETE_REQUEST,\r\n  USER_DELETE_RESET,\r\n  USER_DELETE_SUCCESS,\r\n  USER_DETAILS_FAIL,\r\n  USER_DETAILS_REQUEST,\r\n  USER_DETAILS_SUCCESS,\r